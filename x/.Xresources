! special
*.foreground:   #e8dfd6
*.background:   #16191d
*.cursorColor:  #e8dfd6

! black
*.color0:       #032c36
*.color8:       #065f73

! red
*.color1:       #c2454e
*.color9:       #ef5847

! green
*.color2:       #7cbf9e
*.color10:      #a2d9b1

! yellow
*.color3:       #8a7a63
*.color11:      #beb090

! blue
*.color4:       #2e3340
*.color12:      #61778d

! magenta
*.color5:       #ff5879
*.color13:      #ff99a1

! cyan
*.color6:       #44b5b1
*.color14:      #9ed9d8

! white
*.color7:       #f2f1b9
*.color15:      #f6f6c9

! Rofi colors
rofi.color-enabled: true
rofi.color-window: #16191d, #a2d9b1, #7cbf9e
rofi.color-normal: #16191d, #f6f6c9, #16191d, #7cbf9e, #16191D
rofi.color-active: #16191d, #f6f6c9, #16191d, #7cbf9e, #16191D
rofi.color-urgent: #16191D, #f6f6c9, #16191d, #7cbf9e, #16191D
rofi.separator-style: solid

*.scrollBar: false
*.cursorUnderline: true
*.cursorBlink: true
*.dynamicColors:on

! Sets urxvt to fade when focus is lost
!URxvt*fading: 15
URxvt.internalBorder: 6
