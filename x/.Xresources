! Rofi colors
rofi.color-enabled: true
rofi.color-window: #16191d, #16191d, #7cbf9e
rofi.color-normal: #16191d, #f6f6c9, #16191d, #7cbf9e, #16191d
rofi.color-active: #16191d, #f6f6c9, #16191d, #7cbf9e, #16191d
rofi.color-urgent: #16191D, #f6f6c9, #16191d, #7cbf9e, #16191d
rofi.separator-style: solid

*.scrollBar: false
*.cursorUnderline: true
*.cursorBlink: true
*.dynamicColors:on

URxvt.internalBorder: 8
URxvt.font: xft:Iosevka:Regular:size=10
URxvt.boldFont: xft:Iosevka:Bold:size=10
URxvt.letterSpace: 0
